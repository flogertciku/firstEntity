@model User

<div class="row">
    <div class="col-4">
        <div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">@Model.Name</h5>
    <h6 class="card-subtitle mb-2 text-muted">@Model.Email</h6>
    <p class="card-text">@Model.CreatedAt</p>
    <a asp-action="Delete" asp-route-id="@Model.UserId" class="card-link">Delete</a>
    <a asp-action="Update" asp-route-id="@Model.UserId"  class="card-link">Update</a>
  </div>
</div>
    </div>
</div>